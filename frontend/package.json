{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.12.2",
    "bootstrap": "^5.3.8",
    "framer-motion": "^12.23.24",
    "intersection-observer": "^0.12.2",
    "react": "^19.2.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-lazy-load-image-component": "^1.6.0",
    "react-router-dom": "^7.9.4",
    "react-scripts": "5.0.1",
    "recharts": "^3.2.1",
    "web-vitals": "^2.1.4",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env CI=false GENERATE_SOURCEMAP=false react-scripts build",
    "build:compress": "cross-env CI=false GENERATE_SOURCEMAP=false react-scripts build && npm run compress",
    "build:analyze": "cross-env CI=false GENERATE_SOURCEMAP=false react-scripts build && source-map-explorer \"build/static/js/*.js\"",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compress": "gzipper compress --verbose ./build",
    "lint": "eslint --fix src/**/*.{js,jsx}",
    "format": "prettier --write src/**/*.{js,jsx,css}",
    "preload-fonts": "node scripts/preload-fonts.js",
    "optimize-images": "node scripts/optimize-images.js",
    "analyze": "source-map-explorer \"build/static/js/*.js\"",
    "lighthouse": "lighthouse --view http://localhost:3000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "compression-webpack-plugin": "^10.0.0",
    "cross-env": "^10.1.0",
    "gzipper": "^7.2.0",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^8.0.0",
    "lighthouse": "^11.3.0",
    "prettier": "^3.0.3",
    "source-map-explorer": "^2.5.3",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
